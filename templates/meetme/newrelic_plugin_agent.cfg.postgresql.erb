    - name: <%= @name %>
      host: <%= @host %>
      port: <%= @port.to_i %>
      user: <%= @user %>
      <%- if @password -%>
      password: <%= @password %>
      <%- end -%>
      dbname: <%= @dbname %>
      superuser: <%= @superuser_bool ? 'True' : 'False' %>
      relation_stats: <%= @relation_stats_bool ? 'True' : 'False' %>
